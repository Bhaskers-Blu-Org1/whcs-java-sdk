{
  "id": "general_medical_v1.0_flow",
  "name": "General Medical V1.0 Flow",
  "description": "This flow uses the Concept and Attribute Detection annotators with common configuration as defined in the General Medical V1.0 Profile.",
  "annotatorFlows": [
    {
      "profile": "general_medical_v1.0",
      "flow": {
        "elements": [
          {
            "annotator": {
              "name": "concept_detection",
              "parameters": {
                "libraries": [
                  "umls.2017AA"
                ]
              }
            }
          },
          {
            "annotator": {
              "name": "attribute_detection",
              "parameters": {
                "attribute_set": [
                  "general_medical_v1.0",
                  "general_labs_v1.0"
                ]
              }
            }
          }
        ],
        "async": false
      }
    }
  ]
}